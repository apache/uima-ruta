      <page
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerGlobalPreferencePage"
            id="org.apache.uima.tm.dltk.preferences"
            name="%TextMarkerPreferencePage.name">
      </page>
      <page
            category="org.apache.uima.tm.dltk.preferences"
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerEditorPreferencesPage"
            id="org.apache.uima.tm.dltk.preferences.editor"
            name="%TextMarkerEditorPreferencePage.name">
      </page>
      <page
