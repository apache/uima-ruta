   <extension
         point="org.eclipse.dltk.ui.formatterFactory">
      <engineContribution
            natureId="org.apache.uima.tm.dltk.core.nature">
         <selector
               class="org.eclipse.dltk.core.PreferenceBasedContributionSelector">
            <parameter
                  name="qualifier"
                  value="org.apache.uima.tm.dltk.ui">
            </parameter>
            <parameter
                  name="key"
                  value="formatterId">
            </parameter>
         </selector>
      </engineContribution>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            category="org.apache.uima.tm.dltk.propertyPage"
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerFormatterPreferencePage"
            id="org.apache.uima.tm.dltk.propertyPage.formatter"
            name="TextMarker Formatter">
         <enabledWhen>
            <adapt
                  type="org.eclipse.core.resources.IProject">
               <test
                     property="org.eclipse.core.resources.projectNature"
                     value="org.apache.uima.tm.dltk.core.nature">
               </test>
            </adapt>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.apache.uima.tm.dltk.ui.tmSemanticHighlighting">
      <highlightingExtension
            class="org.apache.uima.tm.dltk.internal.ui.DefaultTextMarkerSemanticHighlightingExtension">
      </highlightingExtension>
   </extension>
      

</plugin>
