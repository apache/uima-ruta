            name="%TextMarkerEditorSyntaxColoringPreferencePage.name">
      </page>
      <page
            category="org.apache.uima.tm.dltk.preferences.editor"
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerSmartTypingPreferencePage"
            id="org.apache.uima.tm.dltk.ui.editor.SmartTyping"
            name="%TextMarkerEditorSmartTypingPreferencePage.name">
      </page>
      <page
            category="org.apache.uima.tm.dltk.preferences.editor"
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerFoldingPreferencePage"
            id="org.apache.uima.tm.dltk.ui.editor.TextMarkerFolding"
            name="%TextMarkerEditorFoldingPreferencePage.name">
      </page>
      <page
        category="org.apache.uima.tm.dltk.preferences"
        class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerTodoTaskPreferencePage"
        id="org.apache.uima.tm.dltk.ui.editor.TodoTasks"
        name="%TextMarkerTaskTags.name">
      </page>
      <page
            category="org.apache.uima.tm.dltk.preferences.editor"
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerEditorAssistancePreferencePage"
            id="org.apache.uima.tm.dltk.ui.editor.Assistence"
            name="%TextMarkerEditorAssistancePreferencePage.name">
      </page>
      <page
            category="org.apache.uima.tm.dltk.preferences"
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerBuilderPreferencePage"
            id="org.apache.uima.tm.dltk.preferences.builder"
            name="Builder">
      </page>
      <page
            category="org.apache.uima.tm.dltk.preferences.editor"
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerFormatterPreferencePage"
            id="org.apache.uima.tm.dltk.preferences.formatter"
            name="Formatter">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            category="org.apache.uima.tm.dltk.propertyPage"
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerBuildpathPropertyPage"
            id="org.apache.uima.tm.dltk.ui.BuildpathProperties"
            name="%TextMarkerBuildPathPropertyPage.name">
         <enabledWhen>
            <adapt
                  type="org.eclipse.core.resources.IProject">
               <test
                     property="org.eclipse.core.resources.projectNature"
                     value="org.apache.uima.tm.dltk.core.nature">
               </test>
            </adapt>
         </enabledWhen>
         <keywordReference
               id="org.eclipse.dltk.ui.buildpath">
         </keywordReference>
      </page>
      <page
            class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerPropertyPage"
            id="org.apache.uima.tm.dltk.propertyPage"
            name="%TextMarkerPropertyPage.name">
         <enabledWhen>
            <adapt
                  type="org.eclipse.core.resources.IProject">
               <test
                     property="org.eclipse.core.resources.projectNature"
                     value="org.apache.uima.tm.dltk.core.nature">
               </test>
            </adapt>
         </enabledWhen>
      </page>
      <page
  			category="org.apache.uima.tm.dltk.propertyPage"
  			class="org.apache.uima.tm.dltk.internal.ui.preferences.TextMarkerTodoTaskPreferencePage"
  			id="org.apache.uima.tm.dltk.propertyPage.todo"
  			name="%TextMarkerTodoPropertyPage.name">
  			<enabledWhen>
    			<adapt type="org.eclipse.core.resources.IProject">
      			<test property="org.eclipse.core.resources.projectNature" value="org.apache.uima.tm.dltk.core.nature"/>
    			</adapt>         
  			</enabledWhen>
	  </page>
   </extension>
   <extension
